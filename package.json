{
  "name": "svelte-spider-chart",
  "version": "0.1.9.7",
  "description": "Interactive Spider Chart component for Svelte 5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package",
    "prepublishOnly": "npm run build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev:test": "vite -c test/vite.config.js",
    "build:test": "vite build -c test/vite.config.js",
    "preview:test": "vite preview -c test/vite.config.js"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "chart",
    "spider-chart",
    "radar-chart",
    "d3",
    "visualization"
  ],
  "author": "Rik Logtenberg",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^5.0.0",
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/svelte": "^4.0.3",
    "@tsconfig/svelte": "^5.0.2",
    "@types/d3": "^7.4.3",
    "@vitest/coverage-v8": "^0.34.6",
    "d3": "^7.8.5",
    "jsdom": "^22.1.0",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^0.34.6"
  }
}